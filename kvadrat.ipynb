    "a = 1\n",
    "b = 10**20\n",
    "c = 1\n",
    "def f(a1, b1, c1):\n",
    "    d = (b1**2 - 4*a1*c1)**0.5\n",
    "    x1 = (-b1 + d)/2\n",
    "    x2 = (-b1 - d)/2\n",
    "    if (c1 == 0) and (x1 != 0 or x2 != 0):\n",
    "        if x1 == 0: \n",
    "            x1 = c1 / x2\n",
    "        if x2 == 0:\n",
    "            x2 = c1 / x1\n",
    "    return x1, x2\n",
    "print(f(a, b, c))\n"
   ]
